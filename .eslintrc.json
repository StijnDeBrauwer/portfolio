{
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:import/typescript"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "project": "tsconfig.json",
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "import",
    "node",
    "eslint-plugin-react",
    "@typescript-eslint"
  ],
  "rules": {
   "no-unused-vars": "off"
  },
  "settings": {
    "import/internal-regex": "^src/.+"
  }
}
